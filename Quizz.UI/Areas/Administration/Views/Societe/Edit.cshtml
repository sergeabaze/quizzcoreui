@model SocieteRequetteViewModel

@{
     Layout = "_LayoutAdministration";
    ViewData["Title"] = "Edit";
}
<h1>Edit</h1>

<h4>Movie</h4>
<hr />
<div class="row">
    <div class="col-md-4">
         <form asp-action="Edit">
             <div asp-validation-summary="ModelOnly" class="text-danger"></div>
             <input type="hidden" asp-for="Id" />
             <div class="form-group">
                <label asp-for="Designation" class="control-label"></label>
                <input asp-for="Designation" class="form-control" />
                <span asp-validation-for="Designation" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="RaisonSociale" class="control-label"></label>
                <input asp-for="RaisonSociale" class="form-control" />
                <span asp-validation-for="RaisonSociale" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
         </form>
    </div>
</div>